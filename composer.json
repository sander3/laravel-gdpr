{
    "name": "dialect/laravel-gdpr-compliance",
    "description": "GDPR compliant data portability and anonymization",
    "keywords": ["laravel", "gdpr", "article 20", "data portability", "user anonymization"],
    "type": "library",
    "homepage": "https://github.com/dialect-katrineholm/laravel-gdpr-compliance",
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Dialect\\Gdpr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dialect\\Gdpr\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dialect Katrineholm",
            "email": "katrineholm@dialect.se"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dialect\\Gdpr\\GdprServiceProvider"
            ]
        }
    }
}
